{
  "version": 1,
  "projects": {
    "awsmscreativepixel": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/awsmscreativepixel",
      "sourceRoot": "apps/awsmscreativepixel/src",
      "prefix": "awsworkspace",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/awsmscreativepixel",
            "index": "apps/awsmscreativepixel/src/index.html",
            "main": "apps/awsmscreativepixel/src/main.ts",
            "polyfills": "apps/awsmscreativepixel/src/polyfills.ts",
            "tsConfig": "apps/awsmscreativepixel/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/awsmscreativepixel/src/favicon.ico",
              "apps/awsmscreativepixel/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "apps/awsmscreativepixel/src/styles.scss",
              "apps/awsmscreativepixel/src/awsms-custom-theme.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/awsmscreativepixel/src/environments/environment.ts",
                  "with": "apps/awsmscreativepixel/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "awsmscreativepixel:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "awsmscreativepixel:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "awsmscreativepixel:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/awsmscreativepixel/tsconfig.app.json",
              "apps/awsmscreativepixel/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/awsmscreativepixel/**"]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/awsmscreativepixel/src/test.ts",
            "tsConfig": "apps/awsmscreativepixel/tsconfig.spec.json",
            "karmaConfig": "apps/awsmscreativepixel/karma.conf.js",
            "polyfills": "apps/awsmscreativepixel/src/polyfills.ts",
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "apps/awsmscreativepixel/src/awsms-custom-theme.scss"
            ],
            "scripts": [],
            "assets": []
          }
        }
      }
    },
    "awsmscreativepixel-e2e": {
      "root": "apps/awsmscreativepixel-e2e",
      "sourceRoot": "apps/awsmscreativepixel-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/awsmscreativepixel-e2e/cypress.json",
            "tsConfig": "apps/awsmscreativepixel-e2e/tsconfig.e2e.json",
            "devServerTarget": "awsmscreativepixel:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "awsmscreativepixel:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/awsmscreativepixel-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/awsmscreativepixel-e2e/**"]
          }
        }
      }
    },
    "awslogin": {
      "projectType": "library",
      "root": "libs/awslogin",
      "sourceRoot": "libs/awslogin/src",
      "prefix": "awsworkspace",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/awslogin/tsconfig.lib.json",
              "libs/awslogin/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/awslogin/**"]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/awslogin/src/test.ts",
            "tsConfig": "libs/awslogin/tsconfig.spec.json",
            "karmaConfig": "libs/awslogin/karma.conf.js"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "awsheader": {
      "projectType": "library",
      "root": "libs/awsheader",
      "sourceRoot": "libs/awsheader/src",
      "prefix": "awsworkspace",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/awsheader/tsconfig.lib.json",
              "libs/awsheader/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/awsheader/**"]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/awsheader/src/test.ts",
            "tsConfig": "libs/awsheader/tsconfig.spec.json",
            "karmaConfig": "libs/awsheader/karma.conf.js"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "karma",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "karma"
    }
  },
  "defaultProject": "awsmscreativepixel"
}
